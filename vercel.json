{
  "version": 2,
  "builds": [
    {
      "src": "client/vite.config.ts",
      "use": "@vercel/vite"
    },
    {
      "src": "server/routes/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server/routes/$1"
    }
  ]
}
